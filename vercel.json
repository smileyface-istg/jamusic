{
  "rewrites": [
    {
      "source": "/api/oembed",
      "destination": "/api/oembed"
    },
    {
      "source": "/track/:id",
      "destination": "/api/track?id=:id"
    },
    {
      "source": "/embed/:id",
      "destination": "/api/embed?id=:id"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}